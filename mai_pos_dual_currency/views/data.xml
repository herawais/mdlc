		<xpath expr="." position="inside">
			<script type="text/javascript" src="/mai_pos_dual_currency/static/src/js/pos_show_currency.js"
			/>
			<script type="text/javascript" src="/mai_pos_dual_currency/static/src/js/ProductItem.js"
			/>
			<script type="text/javascript" src="/mai_pos_dual_currency/static/src/js/PaymentScreenStatus.js"
			/>
